{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@now/node-server",
      "config": {
        "includeFiles": [
          "cache/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/.*", "dest": "src/server.js"}
  ],
  "env": {
    "LISTEN_NOTES_API_KEY": "@listen_notes_api_key"
  }
}
